# Database connection string (PostgreSQL, MySQL, etc.)
DATABASE_URL="prisma+postgres://<your-prisma-connection-url>"

# Google OAuth credentials
GOOGLE_CLIENT_ID="<your-google-client-id>"
GOOGLE_CLIENT_SECRET="<your-google-client-secret>"

# SMTP email configuration
# Use your app password (not your Gmail login password)
EMAIL_SERVER="smtp://<email>@smtp.gmail.com:587"
EMAIL_FROM="GPTree <email@example.com>"

# API keys
GROQ_API_KEY="<your-groq-api-key>"